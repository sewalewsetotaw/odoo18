[options]

# ---------------------------------------------------------
# Path to your custom and community addons for local Odoo.
# Absolute Windows path is required.
# ---------------------------------------------------------
addons_path = C:\odoo_dev\odoo18\addons,C:\odoo_dev\odoo18\custom

# ---------------------------------------------------------
# Database connection settings for local PostgreSQL.
# ---------------------------------------------------------
db_host = localhost
db_port = 5432
db_user = odoo18
db_password = odoo18

# ---------------------------------------------------------
# Master password used to manage databases through the UI.
# ---------------------------------------------------------
admin_passwd = odoo18

# ---------------------------------------------------------
# The port Odoo will listen on.
# Useful when running multiple Odoo versions locally.
# ---------------------------------------------------------
http_port = 9099

# ---------------------------------------------------------
# Path to wkhtmltopdf (used for PDF reports).
# Must point to the executable file on Windows.
# ---------------------------------------------------------
wkhtmltopdf_path = C:\Program Files\wkhtmltopdf\bin\wkhtmltopdf.exe
