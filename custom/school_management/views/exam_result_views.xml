<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="view_exam_management_list" model="ir.ui.view">
        <field name="name">Exam Management List</field>
        <field name="model">exam.management</field>
        <field name="arch" type="xml">
            <list string="Exam Management">
                <field name="name"/>
                <field name="course_id"/>
                <field name="section_id"/>
                <field name="academic_year"/>
                <field name="exam_date"/>
                <field name="max_marks"/>
                <field name="passing_marks"/>
            </list>
        </field>
    </record>
    <record id="view_exam_management_form" model="ir.ui.view">
        <field name="name">Exam Management Form</field>
        <field name="model">exam.management</field>
        <field name="arch" type="xml">
            <form string="Exam Management">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="course_id"/>
                        <field name="section_id"/>
                        <field name="academic_year"/>
                        <field name="exam_date"/>
                        <field name="max_marks"/>
                        <field name="passing_marks"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_result_management_list" model="ir.ui.view">
        <field name="name">Result Management List</field>
        <field name="model">result.management</field>
        <field name="arch" type="xml">
            <list string="Result Management">
                <field name="name"/>
                <field name="exam_id"/>
                <field name="student_id"/>
                <field name="marks_obtained"/>
                <field name="grade"/>
                <field name="status"/>
            </list>
        </field>
    </record>
    <record id="view_result_management_form" model="ir.ui.view">
        <field name="name">Result Management Form</field>
        <field name="model">result.management</field>
        <field name="arch" type="xml">
            <form string="Result Management">
                <sheet>
                    <group>
                        <field name="name" readonly="1"/>
                        <field name="exam_id"/>
                        <field name="student_id"/>
                        <field name="marks_obtained"/>
                        <field name="grade"/>
                        <field name="status"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>